{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "gravel",
  "license": "MIT",
  "version-semver": "0.1.0",
  "maintainers": [
    "albin-johansson"
  ],
  "builtin-baseline": "31a159c1cae2bf905299085d9ef01bdfea0ca7b8",
  "dependencies": [
    "assimp",
    "boost-assert",
    "boost-predef",
    "boost-stacktrace",
    "entt",
    "fmt",
    {
      "name": "glad",
      "features": [
        "extensions"
      ]
    },
    "glm",
    {
      "name": "imgui",
      "features": [
        "sdl2-binding",
        "opengl3-binding",
        "docking-experimental",
        "vulkan-binding"
      ]
    },
    "imguizmo",
    "magic-enum",
    {
      "name": "sdl2",
      "features": [
        "vulkan"
      ]
    },
    "spdlog",
    "stb",
    "vulkan",
    "vulkan-memory-allocator"
  ]
}